{
    "name": "bolt/bolt-applications",
    "description": "Lorem ipsum",
    "type": "bolt-extension",
    "keywords" : [ ],
    "require": {
        "bolt/bolt": ">=2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Januszewski",
            "email": "e.januszewski@gmail.com"
        }
    ],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\EJanuszewski\\BoltApplications\\": [
                "",
                "src"
            ]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Bolt\\Extension\\EJanuszewski\\BoltApplications\\Tests\\": "tests",
            "Bolt\\Tests\\" : "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "assets"
    }
}
